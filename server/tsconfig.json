{
   "compilerOptions": {
      "esModuleInterop": true,
      "noImplicitAny": true,
      
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true
   },
   "exclude": ["node_modules", "__tests__", "scripts"]
}
